{
  "name": "night-owl-x",
  "displayName": "Night Owl X",
  "description": "A VS Code theme for the night owls out there.",
  "version": "1.0.0",
  "publisher": "lagasi",
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/lagasi/night-owl-vscode-theme"
  },
  "keywords": [
    "Theme",
    "Dark Theme",
    "Night Owl",
    "Night Theme"
  ],
  "scripts": {
    "build": "vsce package -o night-owl-x.vsix",
    "install": "code --install-extension night-owl-x.vsix",
    "bi": "npm run build && npm run install",
    "publish": "vsce publish"
  },
  "galleryBanner": {
    "color": "#061526",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Themes"
  ],
  "icon": "owl-icon.png",
  "contributes": {
    "themes": [
      {
        "label": "Night Owl X",
        "uiTheme": "vs-dark",
        "path": "./themes/Night Owl-color-theme.json"
      }
    ]
  }
}
